#include "holberton.h"

/**
 * main - check the code for Holberton School students.
 *
 * Return: Always 0.
 */
int _strcmp(char *s1, char *s2)
{
	int i,j;

	i = 0;
	while ((s1[i] && s2[i]) != '\0')
	{
		if (s1[i] != s2[i])
		{
			j = s1[i] - s2[i];
				return (j);
		}
		else
		{
			return (0);
		}
				i++;
	}
}
