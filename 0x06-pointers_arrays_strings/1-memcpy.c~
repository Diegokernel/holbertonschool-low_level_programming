#include "holberton.h"

/**
 * *_memeset - prints buffer in hexa
 * @dest: the address of memory to print
 * @src: the size of the memory to print
 * @n: check
 * Return: mem.
 */
char *_memcpy(char *dest, char *src, unsigned int n)
{
        unsigned int i;

        i = 0;
        while (i++, i < n)
		dest[i] = src[i];
        return (dest);
}
